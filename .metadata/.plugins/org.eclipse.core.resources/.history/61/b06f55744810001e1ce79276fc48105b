package bai2;
public class Demo {
    int count = 1;

    public Demo() {

    }

    public void visit() {
        count++;
        this.print();
    }

    public void print() {
        System.out.println("count = " + count);
    }

    public static void main(String[] args) {
        Demo c1 = new Demo();
        c1.visit();
        Demo c2 = new Demo();
        c2.visit();
        Demo c3 = new Demo();
        c3.visit();
    }
}
